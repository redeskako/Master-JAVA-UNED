<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4"
	xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>PeluqueriaRizos2</display-name>
	<servlet>
		<display-name>ProcesaLogado</display-name>
		<servlet-name>ProcesaLogado</servlet-name>
		<servlet-class>
			es.rizos.ServletsControladores.ProcesaLogado
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ServletCita</servlet-name>
		<servlet-class>
			es.rizos.ServletsControladores.ServletCita
		</servlet-class>
	</servlet>
	<servlet>
		<description></description>
		<display-name>ProcesoClientes</display-name>
		<servlet-name>ProcesoClientes</servlet-name>
		<servlet-class>
			es.rizos.ServletsControladores.ProcesoClientes
		</servlet-class>
	</servlet>
	<servlet>
		<description></description>
		<display-name>ProcesoDatosClientes</display-name>
		<servlet-name>ProcesoDatosClientes</servlet-name>
		<servlet-class>
			es.rizos.ServletsControladores.ProcesoDatosClientes
		</servlet-class>
	</servlet>
	<servlet>
		<description></description>
		<display-name>ProcesaLogado2</display-name>
		<servlet-name>ProcesaLogado2</servlet-name>
		<servlet-class>
			es.rizos.ServletsControladores.ProcesaLogado2
		</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ProcesaLogado</servlet-name>
		<url-pattern>/ProcesaLogado</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProcesoClientes</servlet-name>
		<url-pattern>/ProcesoClientes</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProcesoDatosClientes</servlet-name>
		<url-pattern>/ProcesoDatosClientes</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletCita</servlet-name>
		<url-pattern>/ServletCita</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProcesoClientes</servlet-name>
		<url-pattern>/ProcesoClientes</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProcesoDatosClientes</servlet-name>
		<url-pattern>/ProcesoDatosClientes</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProcesaLogado2</servlet-name>
		<url-pattern>/ProcesaLogado2</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>FiltroLogado</filter-name>
		<filter-class>es.rizos.librerias.FiltroLogado</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>FiltroLogado</filter-name>
		<url-pattern>/GestionaCli.jsp</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>FiltroLogado</filter-name>
		<url-pattern>/GestionaAdmin.jsp</url-pattern>
	</filter-mapping>
	<welcome-file-list>
		<welcome-file>Bienvenida.jsp</welcome-file>
	</welcome-file-list>
	<resource-ref>
		<res-ref-name>jdbc/rizos_bd</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
</web-app>