<?xml version="1.0" encoding="UTF-8"?>
 <web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  <display-name>PeluqueriaRizos</display-name>
	<servlet>
		<display-name>ProcesaLogado</display-name>
		<servlet-name>ProcesaLogado</servlet-name>
		<servlet-class>es.rizos.ServletsControladores.ProcesaLogado</servlet-class>
	</servlet>

<servlet>
  <display-name>ProcesoClientes</display-name>
  <servlet-name>ProcesoClientes</servlet-name>
  <servlet-class>es.rizos.librerias.ProcesoClientes</servlet-class>
</servlet>
<servlet>
	<display-name>ProcesoDatosClientes</display-name>
	<servlet-name>ProcesoDatosClientes</servlet-name>
	<servlet-class>es.rizos.librerias.ProcesoDatosClientes</servlet-class>
</servlet>
   <servlet>
        <servlet-name>ServletCita</servlet-name>
        <servlet-class>es.rizos.ServletsControladores.ServletCita</servlet-class>
    </servlet>
 <servlet-mapping>
  <servlet-name>ProcesaLogado</servlet-name>
  <url-pattern>/ProcesaLogado</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ProcesoClientes</servlet-name>
  <url-pattern>/ProcesoClientes</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
 	<servlet-name>ProcesoDatosClientes</servlet-name>
 	<url-pattern>/ProcesoDatosClientes</url-pattern>
 </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletCita</servlet-name>
        <url-pattern>/ServletCita</url-pattern>
    </servlet-mapping>
 <welcome-file-list>
  <welcome-file>frames.html</welcome-file>
 </welcome-file-list>
 </web-app>