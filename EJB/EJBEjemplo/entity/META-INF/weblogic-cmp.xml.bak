<!DOCTYPE weblogic-rdbms-jar PUBLIC 
 '-//BEA Systems, Inc.//DTD WebLogic 6.0.0 EJB 1.1 RDBMS Persistence//EN'
 'http://www.bea.com/servers/wls600/dtd/weblogic-rdbms11-persistence-600.dtd'>

<weblogic-rdbms-jar>
	<weblogic-rdbms-bean>
		<ejb-name>CMPClient</ejb-name>
		<data-source-name>MySQLDataSource</data-source-name>
		<table-name>CLIENT</table-name>
		
		<field-map>
			<cmp-field>clientId</cmp-field>
			<dbms-column>CLIENTID</dbms-column>
		</field-map>
		<field-map>
			<cmp-field>name</cmp-field>
			<dbms-column>NAME</dbms-column>
		</field-map>
		<field-map>
			<cmp-field>address</cmp-field>
			<dbms-column>ADDRESS</dbms-column>
		</field-map>
		<field-map>
			<cmp-field>town</cmp-field>
			<dbms-column>TOWN</dbms-column>
		</field-map>
	</weblogic-rdbms-bean>
</weblogic-rdbms-jar>